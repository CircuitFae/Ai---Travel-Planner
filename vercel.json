{
  "builds": [
    {
      "src": "backend.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend.py"
    }
  ]
}
